<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Text?>

<Pane prefHeight="50.0" prefWidth="600.0" style="-fx-background-color: DEEFF5;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.harmoniamusic.NavigatorController">
   <children>

      <ImageView fitHeight="35.0" fitWidth="50.0" layoutX="540.0" layoutY="10.0" onMouseClicked="#goToSettings" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@profile.png" />
         </image>
      </ImageView>
      <Text layoutX="255.0" layoutY="30.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Harmonia Music" />

      <ImageView fitHeight="35.0" fitWidth="50.0" layoutX="80.0" layoutY="10.0" onMouseClicked="#createSong" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@plus.png" />
         </image>
      </ImageView>

         <ImageView fitHeight="35.0" fitWidth="50.0" layoutX="20.0" layoutY="10.0" onMouseClicked="#goToHome" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@home.png" />
            </image>
         </ImageView>
   </children>
</Pane>
