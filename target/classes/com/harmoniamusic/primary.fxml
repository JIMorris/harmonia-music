      <Label text="Sign in to Harmonia Music">
         <VBox.margin>
            <Insets />
         </VBox.margin></Label>
      <Label text="Username">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Label>
      <TextField />
      <TextField />
      <Button fx:id="primaryButton" onAction="#switchToSecondary" prefHeight="25.0" prefWidth="112.0" text="Sign in" />
